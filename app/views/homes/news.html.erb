<% @news.each do |key, values| %>
  <div class = "row">
    <%= image_tag("apple-touch-icon.png", class: 'thumbnail-image') %>
    <!-- <p id="source">News from <%= key %></p> -->
    <ul class="thumbnail-grid news_group">
      <% values.each do |value| %>
        <li class="thumbnail-list-item">
          <!-- <%= image_tag("apple-touch-icon.png", class: 'thumbnail-image') %> -->
          <h3 id="article-header"> <%= link_to value["title"], value["url"], target: :_blank  %> </h3>
          <p id=""><%= value["description"] %></p>
        </li>
      <% end %>
    </ul>
  </div>
<% end %>
